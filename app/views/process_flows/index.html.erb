<p id="notice"><%= notice %></p>

<h1>Process Flows</h1>

<table>
  <thead>
    <tr>
      <th>Step number</th>
      <th>Step name</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @process_flows.each do |process_flow| %>
      <tr>
        <td><%= process_flow.step_number %></td>
        <td><%= process_flow.step_name %></td>
        <td><%= process_flow.team %></td>
        <td><%= link_to 'Show', process_flow %></td>
        <td><%= link_to 'Edit', edit_process_flow_path(process_flow), class: "btn btn-info", style: "color: white" %></td>
        <td><%= link_to 'Destroy', process_flow, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-warning", style: "color: white" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Process Flow', new_process_flow_path %>
